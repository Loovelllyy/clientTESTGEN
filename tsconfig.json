{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "es6",
    "target": "es5",
    "allowJs": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "typeRoots": ["./@types", "./node_modules/@types", "./media.d.ts", "./d3.d.ts",
      "./node_modules/node-fetch/@types/node-fetch.d.ts"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "@emotion/react",

  },
  "paths": {
    "~media": "src/media"
  }
}